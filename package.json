{
    "name": "shoppingo",
    "version": "0.2.4",
    "private": true,
    "repository": "git@github.com:igor-siergiej/shoppingo.git",
    "author": "Igor Siergiej <igorsiergiej@gmail.com>",
    "license": "MIT",
    "scripts": {
        "start:web": "yarn workspace @shoppingo/web start",
        "start:web:with-mock": "dotenv -e .env -- yarn workspace @shoppingo/web start:with-mock",
        "start:api": "dotenv -e .env -- yarn workspace @shoppingo/api start",
        "start": "yarn start:web & yarn start:api",
        "start:with-mock": "yarn start:web:with-mock & yarn start:api",
        "lint": "eslint . --ext .ts,.tsx",
        "lint:fix": "eslint . --ext .ts,.tsx --fix"
    },
    "workspaces": [
        "packages/*"
    ],
    "devDependencies": {
        "@eslint/js": "^9.33.0",
        "@eslint/json": "^0.13.1",
        "@stylistic/eslint-plugin": "^5.2.3",
        "@typescript-eslint/eslint-plugin": "^8.39.0",
        "@typescript-eslint/parser": "^8.39.0",
        "dotenv-cli": "^10.0.0",
        "eslint": "^9.33.0",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jest": "^29.0.1",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-unused-imports": "^4.1.4",
        "eslint-plugin-use-encapsulation": "^1.3.1",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.39.0"
    },
    "packageManager": "yarn@4.9.2"
}
